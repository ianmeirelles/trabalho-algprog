int usuario(){
  int i, j, matriz[6][5];
  printf("WiSARD Original\n\nDigite uma letra em formato de uma matriz 6x5(1:Preto;0:Branco):\n\n");
  for(i=0;i<6;i++) {
    for(j=0;j<5;j++) {
      printf("Digite a cor na linha %d e na coluna %d: ", i, j);
      scanf("%d", &matriz[i][j]);
    }
  }
  return matriz[6][5];
}
